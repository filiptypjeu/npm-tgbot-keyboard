{
    "name": "tgbot-keyboard",
    "version": "1.2.0",
    "description": "",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "format": "prettier --write \"(src|typings)/**/*.ts\"",
        "link": "npm run build && npm link",
        "lint": "eslint src/**/* --ext .ts",
        "lint-fix": "eslint src/**/* --ext .ts --fix",
        "postversion": "git push && git push --tags",
        "prepare": "npm run build",
        "prepublishOnly": "npm test && npm run lint",
        "preversion": "npm run format && npm run lint",
        "test": "jest --config jestconfig.json",
        "test-coverage": "jest --config jestconfig.json --coverage",
        "version": "npm run format && git add -A src"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:filiptypjeu/npm-tgbot-keyboard.git"
    },
    "homepage": "https://github.com/filiptypjeu/npm-tgbot-keyboard",
    "author": "Filip Stenbacka",
    "license": "ISC",
    "types": "dist/index.d.ts",
    "dependencies": {
        "@types/node-localstorage": "^1.3.0",
        "@types/node-telegram-bot-api": "^0.53.3",
        "node-localstorage": "^2.2.1",
        "node-telegram-bot-api": "^0.56.0",
        "tgbot-helpers": "^3.4.0"
    },
    "devDependencies": {
        "@types/jest": "^27.4.0",
        "@typescript-eslint/eslint-plugin": "^4.29.3",
        "@typescript-eslint/parser": "^4.29.3",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "jest": "^27.4.7",
        "prettier": "^2.5.1",
        "ts-jest": "^27.1.3",
        "typescript": "^4.5.4"
    },
    "files": [
        "dist"
    ],
    "keywords": [
        "keyboard",
        "telegram",
        "tg",
        "bot",
        "tgbot"
    ]
}
